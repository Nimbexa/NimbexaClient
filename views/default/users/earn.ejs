    <body>
      <!-- Start wrapper-->
 <div id="wrapper">
  <%- include('../components/sidebar') %>
  <%- include('../components/topnav') %>
  <div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
      <!---- ALERTS ----->
    <%- include('../components/alert') %>
    <% if (settings.shareus.enabled == true) { %>
      <div class="row mt-3">
        <div class="col-lg-6">
          <div class="card">
            <div class="card-body">
              <div class="card-title">ShareUS.io</div>
              <hr>
              <div>
                <div>
                  <div>
                    <div id="generate-shareus-panel">
                      <p style="color:white; text-align: start;">
                        You will get <%= settings.shareus.coins %> coins after you successfully complete a ShareUs.io link!<br>
                        <br>
                        <button class="btn btn-light btn-round px-5" id="generateBtn">
                          Generate Link
                        </button>
                      </p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% if (settings.ads.enabled == true) { %>
          <div class="col-lg-6">
            <div class="card">
               <div class="card-body">
               <div style="cursor: pointer;" onclick="location.href = '<%= settings.ads.ads[0].redirect %>';" class="card-title"><%= settings.ads.ads[0].title %></div>
               <hr>
                <img onclick="location.href = '<%= settings.ads.ads[0].redirect %>';" style="border-radius: 15px; cursor: pointer; max-width: 100%;" src="<%= settings.ads.ads[0].image %>">
             </div>
             </div>
          </div>
          <% } %>
      </div>
    
      <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
      <script>
        const generateBtn = document.getElementById('generateBtn');
    
        generateBtn.addEventListener('click', async () => {
          try {
            const response = await fetch('/earn/shareus/generate');
            const data = await response.json();
    
            if (response.ok) {
              Swal.fire({
                title: 'Success',
                text: 'ShareUs link generated successfully.',
                icon: 'success',
                html: `Your ShareUs link: <a style="color: aqua" href="${data.link}"> Link</a>`
              });
            } else {
              Swal.fire({
                title: 'Error',
                text: 'Error generating ShareUs link. Please try again later.',
                icon: 'error'
              });
            }
          } catch (error) {
            console.error('Error generating ShareUs link:', error);
            Swal.fire({
              title: 'Error',
              text: 'Error generating ShareUs link. Please try again later.',
              icon: 'error'
            });
          }
        });
      </script>
    <% } %>
    
